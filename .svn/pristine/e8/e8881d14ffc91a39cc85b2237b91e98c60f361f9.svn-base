<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.tedu.entity.Account"	table="account" dynamic-insert="true" dynamic-update="true">
		<id name="accountId" type="int" column="accountId">
		    <generator class="identity"/>
		</id>
		<property name="accountname" type="string"	column="accountname" >	</property>
		<property name="type" type="string"	column="type">	</property>
		<property name="note" type="string"	column="note">	</property>
		<property name="userId" type="int"	column="userId">	</property>
		<property name="money" type="int"	column="money">	</property>

	</class>

</hibernate-mapping>
