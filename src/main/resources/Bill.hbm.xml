<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.tedu.entity.Bill"	table="bill" dynamic-insert="true" dynamic-update="true">
		<id name="billId" type="int" column="billId">
		    <generator class="identity"/>
		</id>
		<property name="createdate" type="date"	column="createdate" >	</property>
		<property name="type" type="string"	column="type">	</property>
		<property name="note" type="string"	column="note">	</property>
		<property name="payAccount" type="int"	column="payAccount">	</property>
		<property name="receiveAccount" type="int"	column="receiveAccount">	</property>
		<property name="bookId" type="int"	column="bookId">	</property>
		<property name="num" type="int"	column="num">	</property>








	</class>
	

</hibernate-mapping>
