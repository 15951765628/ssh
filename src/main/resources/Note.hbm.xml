<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.tedu.entity.Note"	table="note" dynamic-insert="true" dynamic-update="true">
		<id name="noteId" type="int" column="noteId">
		    <generator class="identity"/>
		</id>
		<property name="note" type="string"	column="note" >	</property>
		<property name="createdate" type="date"	column="createdate">	</property>
		<property name="enddate" type="date"	column="enddate">	</property>
		<property name="startdate" type="date"	column="startdate">	</property>		
		<property name="username" type="string"	column="username">	</property>		
		
	</class>
</hibernate-mapping>
