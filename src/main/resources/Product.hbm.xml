<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.tedu.entity.Product"	table="product" dynamic-insert="true" dynamic-update="true">
		<id name="productId" type="int" column="productId">
		    <generator class="identity"/>
		</id>
		<property name="name" type="string"	column="name" >	</property>
		<property name="num" type="int"	column="num">	</property>
		<property name="note" type="string"	column="note">	</property>
		<property name="userId" type="int"	column="userId">	</property>
		<property name="billId" type="int"	column="billId">	</property>
		<property name="payaccountId" type="int"	column="payaccountId">	</property>
		<property name="price" type="int"	column="price">	</property>
		<property name="totalMoney" type="int"	column="totalMoney">	</property>
		<property name="rate" type="float"	column="rate">	</property>
		<property name="startdate" type="date"	column="startdate">	</property>
		<property name="createdate" type="date"	column="createdate">	</property>
		<property name="enddate" type="date"	column="enddate">	</property>
		<property name="type" type="int"	column="type">	</property>
		
		
	</class>
	
	

</hibernate-mapping>
